  "dataBindings": [
    {
      "attr": "angle",
      "valueType": "Number",
      "defaultValue": 0
    },
    {
      "attr": "animation",
      "valueType": "Function",
      "defaultValue": "__ht__function(data,valueObj) {\r\n\r\n    var animationEnable = data.a('animationEnable');\r\n    var animationDisable = data.a('animationDisable');\r\n    var animationDefault = data.a('animationDefault');\r\n\r\n    if(animationEnable){\r\n        if(valueObj == data.a('animationEnable')){\r\n            //启动动画\r\n            data.a('color',data.a('enableColor'));\r\n            data.setAnimation({\r\n                angleAdd: {\r\n                    property: \"angle\",\r\n                    accessType:\"attr\",\r\n                    from: 360, \r\n                    to: 0,\r\n                    repeat:true,\r\n                    interval:1,\r\n                    easing :\"Linear\",\r\n                    frames:360\r\n                },\r\n                start: [\"angleAdd\"]\r\n            });\r\n        }\r\n        if(valueObj == null){\r\n             data.a('color',data.a('nullColor'));\r\n        }\r\n    }\r\n    if(animationDisable){\r\n        if(valueObj == data.a('animationDisable')){\r\n            //停止动画\r\n            data.a('color',data.a('disableColor'));\r\n            data.setAnimation(null);\r\n        }\r\n        if(valueObj == null){\r\n             data.a('color',data.a('nullColor'));\r\n        }\r\n    }\r\n    if(!(animationEnable || animationDisable)){\r\n        if(animationDefault == false){\r\n            //停止动画\r\n            data.a('color',data.a('disableColor'));\r\n            data.setAnimation(null);\r\n        }else{\r\n            //启动动画\r\n            data.a('color',data.a('enableColor'));\r\n            data.setAnimation({\r\n                angleAdd: {\r\n                    property: \"angle\",\r\n                    accessType:\"attr\",\r\n                    from: 360, \r\n                    to: 0,\r\n                    repeat:true,\r\n                    interval:1,\r\n                    easing :\"Linear\",\r\n                    frames:360\r\n                },\r\n                start: [\"angleAdd\"]\r\n            });\r\n        }\r\n    }\r\n}"
    },
